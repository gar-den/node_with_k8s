{
  "name": "node_with_k8s",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d ormconfig.json",
    "build": "echo 'build server...' && rimraf ./lib && tsc -b .",
    "server": "rimraf ./lib && tsc && node lib/server.js",
    "db:sync": "typeorm schema:sync",
    "db:migrate:generate": "typeorm migration:generate -d migration -n",
    "db:migrate:revert": "typeorm migration:revert",
    "db:migration": "typeorm migration:run"
  },
  "author": "Garden Lee <prepella3@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@typegoose/typegoose": "^9.9.0",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "body-parser": "^1.20.0",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "graphql": "^15.3.0",
    "graphql-depth-limit": "^1.1.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.0",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21"
  },
  "repository": "https://github.com/gar-den/node_with_k8s.git"
}
